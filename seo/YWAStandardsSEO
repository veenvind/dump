     <input type="hidden" name="web"    id="web"    value="Lego" />
     <input type="text"   name="query"  id="query"  size="15" />
     <br />
     <select name="webOrSite">
       <option value="Lego" /> Lego  
       <option value="all" /> all TWiki 
     </select>
     <br />
     <input type="radio" name="textOrTopic" value="topic" /> topic  
     <input type="radio" name="textOrTopic" value="text"  checked /> content 
     <br />
