   <!-- "Backyard" search for "text"; TWiki search for "topic" -->
   <form name="SearchFormTop" action="http://twiki.corp.yahoo.com/search/Lego" onsubmit="searchActionTop();" >
     <input type="hidden" name="search" id="search" value="" />
     <input type="hidden" name="site"   id="site"   value="twiki" />
     <input type="hidden" name="type"   id="type"   value="regex" />
     <input type="hidden" name="scope"  id="scope"  value="topic" />
     <input type="hidden" name="web"    id="web"    value="Lego" />
     <input type="text"   name="query"  id="query"  size="25" />
     <select name="webOrSite">
       <option value="Lego" /> Lego  
       <option value="all" /> all TWiki 
     </select>
     <input type="radio" name="textOrTopic" value="topic" /> topic  
     <input type="radio" name="textOrTopic" value="text"  checked /> content 
     <input type="submit" name="Go" value="Search"  />
   </form>
